
@{
    ViewData["Title"] = "Index";
    var Nombre = (String)ViewData["Nombre"];
}

<div class="text-right">
    <h3>Administrador: @Nombre</h3>
    <a class="btn btn-primary btn-sm ml-auto" asp-area="" asp-controller="Home" asp-action="Index">Logout</a>
</div>

<div class="home_content text-center">
    <h2>Modulo Administrador</h2>
</div>

<div class="home_content text-center">
    <a class="btn btn-primary btn-sm ml-auto" asp-area="" asp-controller="Maestro" asp-action="Index">Manejo maestros</a>
    <a class="btn btn-primary btn-sm ml-auto" asp-area="" asp-controller="Estudiante" asp-action="Index">Manejo estudiantes</a>
    <a class="btn btn-primary btn-sm ml-auto" asp-area="" asp-controller="Carrera" asp-action="Index">Crear Carreras</a>

    <a class="btn btn-primary btn-sm ml-auto" asp-area="" asp-controller="MaestroCarrera" asp-action="Index">Asignar Maestros</a>
    <a class="btn btn-primary btn-sm ml-auto" asp-area="" asp-controller="AlumnoCarrera" asp-action="Index">Asignar Alumnos</a>
    <a class="btn btn-primary btn-sm ml-auto" asp-area="" asp-controller="Publicacion" asp-action="Create">Programar Aviso</a>
    <a class="btn btn-primary btn-sm ml-auto" asp-area="" asp-controller="ModuloMaestro" asp-action="Actividades">Cargar CSV</a>
</div>
