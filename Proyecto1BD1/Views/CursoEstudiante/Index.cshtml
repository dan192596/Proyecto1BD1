@model List<List<String>>
@{
    ViewData["Title"] = "Index";
}

<h2>Maestro</h2>

<p>
    <a asp-action="Create">Crear nuevo</a>
</p>
<table class="table">
    <thead>
        <tr>
            <td>Alumno</td>
            <td>Maestro</td>
            <td>Carrera</td>
            <td>Curso</td>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
        <tr>
            <td>@(item.ElementAt(0))</td>
            <td>@(item.ElementAt(1))</td>
            <td>@(item.ElementAt(2))</td>
            <td>@(item.ElementAt(3))</td>
            <td>
                @Html.ActionLink("Delete", "Delete", new {carnet =item.ElementAt(0), registro = item.ElementAt(1), carrera = item.ElementAt(2), curso = item.ElementAt(3) })
            </td>
        </tr>
        }
    </tbody>
